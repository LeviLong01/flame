{  
   "configurations":[  
      {  
         "name":"prod",
         "policies":[  
            {  
               "name":"HPX_redirectToConsent.xml",
               "tokens":[  
                  {  
                     "xpath":"/AssignMessage/Set/Headers/Header[@name='Location']",
                     "value":"https://__HOST__/openid/index?sessionid={sessionid}"
                  }
               ]
            },
            {  
               "name":"HPX_AssignSendSMSAPIRequest.xml",
               "tokens":[  
                  {  
                     "xpath":"/AssignMessage/Set/Headers/Header[@name='apikey']",
                     "value":"__APIKEY__"
                  }
               ]
            },
			{  
               "name":"HPX_AssignlogoutRedircetUri.xml",
               "tokens":[  
                  {  
                     "xpath":"/AssignMessage/Set/QueryParams/QueryParam[@name='logoutRedircetUri']",
                     "value":"https://__HOST__/openid/index"
                  }
               ]
            },
            {  
               "name":"HPX_AddVariablesToTargetRequest.xml",
               "tokens":[  
                  {  
                     "xpath":"/AssignMessage/Add/QueryParams/QueryParam[@name='hostName']",
                     "value":"https://__HOST__"
                  }
               ]
            },
            {  
               "name":"HPX_CheckAlreadyConsented.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/AssignMessage/Set/Headers/Header[@name='Authorization']",
                     "value":"ClientCredential __AUTH__"
                  },
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity/consents/validate"
                  }
               ]
            },
            {  
               "name":"HPX_CheckMSISDNExists.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity-users/v1/users/{msisdn}"
                  }
               ]
            },
            {  
               "name":"HPX_CheckUserExists.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity-users/v1/users/{username}"
                  }
               ]
            },
            {  
               "name":"HPX_CreateUser.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity-users/v1/users"
                  }
               ]
            },
            {  
               "name":"HPX_GenerateAuthCode.Callout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/oauth/v2/code"
                  }
               ]
            },
            {  
               "name":"HPX_GetUserApps.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity/consents"
                  },
                  {  
                     "xpath":"/ServiceCallout/Request/Set/Headers/Header[@name='Authorization']",
                     "value":"ClientCredential __AUTH__"
                  }
               ]
            },
            {  
               "name":"HPX_GetUserProfileFromUserManagement.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity-users/v1/users/{userid}"
                  }
               ]
            },
            {  
               "name":"HPX_ResetPassword.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity-users/v1/users/{reset_username}/password"
                  }
               ]
            },
            {  
               "name":"HPX_RevokeUserConsent.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity/consents/{request.queryparam.consentid}"
                  },
                  {  
                     "xpath":"/ServiceCallout/Request/Set/Headers/Header[@name='Authorization']",
                     "value":"ClientCredential __AUTH__"
                  }
               ]
            },
            {  
               "name":"HPX_SendSMS.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/v1/token/sms/{msisdn}"
                  }
               ]
            },
            {  
               "name":"HPX_ServiceCallout.IdentifyUser.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/authenticate/identify"
                  }
               ]
            },
            {  
               "name":"HPX_ServiceCallout.ValidateUser.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity-users/v1/users/authenticate"
                  }
               ]
            },
            {  
               "name":"HPX_SetPassword.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity-users/v1/users/{username}/password"
                  }
               ]
            },
            {  
               "name":"HPX_ValidatePin.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/v1/token/sms/{msisdn}/{request.formparam.pin}"
                  },
                  {  
                     "xpath":"/ServiceCallout/Request/Set/Headers/Header[@name='apikey']",
                     "value":"__APIKEY__"
                  }
               ]
            },
            {  
               "name":"HPX_UpdateUserProfile.ServiceCallout.xml",
               "tokens":[  
                  {  
                     "xpath":"/ServiceCallout/HTTPTargetConnection/URL",
                     "value":"https://__HOST__/identity-users/v1/users/{request.formparam.email}"
                  }
               ]
            }
         ],
